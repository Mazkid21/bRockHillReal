<div id="make-3D-space">
    <div id="product-card">
        <div id="product-front">
            <div class="shadow"></div>
            <img src="{{property.0.Media.0.MediaURL}}" alt="" />
            <div class="image_overlay"></div>
            <div id="view_details">View property</div>
            <div class="stats">
                <div class="stats-container">
                    <span class="product_price">{{property.0.ListPrice}}</span>
                    <span class="product_name">{{property.0.UnparsedAddress}}</span>
                    <p>Men's running shirt</p>

                    <div class="product-options">
                        <strong>SIZES</strong>
                        <span>XS, S, M, L, XL, XXL</span>
                        <strong>COLORS</strong>
                        <div class="colors">
                            <div class="c-blue"><span></span></div>
                            <div class="c-red"><span></span></div>
                            <div class="c-white"><span></span></div>
                            <div class="c-green"><span></span></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="product-back">
            <div class="shadow"></div>
            <div id="carousel">
                <ul>
                    <li><img src="{{property.0.Media.1.MediaURL}}" alt="" /></li>
                    <li><img src="{{property.0.Media.2.MediaURL}}" alt="" /></li>
                    <li><img src="{{property.0.Media.3.MediaURL}}" alt="" /></li>
                </ul>
                <div class="arrows-perspective">
                    <div class="carouselPrev">
                        <div class="y"></div>
                        <div class="x"></div>
                    </div>
                    <div class="carouselNext">
                        <div class="y"></div>
                        <div class="x"></div>
                    </div>
                </div>
            </div>
            <div id="flip-back">
                <div id="cy"></div>
                <div id="cx"></div>
            </div>
        </div>
    </div>
</div>

