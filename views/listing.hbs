<head>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="css/card.css">
    <script src="js/card.js"></script>
</head>

<div>

    <h1>TESTTTTT</h1>
    <button id="search" type="search" placeholder="Search Craig's Jobs">BUTTON TEST</button>

    <div id="cat-list-template">
        <script type="text/x-handlebars-template" id="image-template">
<div>
  {{#each property }}
<p>{{this.UnparsedAddress}}</p>
   <br>
     <p>>{{Directions}}</p>
     <br>
     <img src="{{this.Media.0.MediaURL}}" alt="">
  {{/each}}
  </div>
</script>

    </div>

</div>


<section id='works' class="s-works">

    <div class="intro-wrap">

        <div class="row section-header has-bottom-sep light-sep" data-aos="fade-up">
            <div class="col-full">
                <h3 class="subhead">Recent Lisitings</h3>
                <h1 class="display-2 display-2--light">We love what we do, check out some of our latest listings</h1>
            </div>
        </div> <!-- end section-header -->

    </div> <!-- end intro-wrap -->


    <div class="row works-content">
        <div class="col-full masonry-wrap">
            <div class="masonry">
                {{#each property}}
                <div class="masonry__brick" data-aos="fade-up">
                    <div class="item-folio">
                        <div class="item-folio__thumb">

                            <a href="{{this.MediaURL}}" class="thumb-link" title="Shutterbug" data-size="1050x700">
                                <img src="{{this.Media.0.MediaURL}}" alt="">
                            </a>
                        </div>

                        <div class="item-folio__text">
                            <h3 class="item-folio__title">
                                {{this.UnparsedAddress}}
                            </h3>
                            <p class="item-folio__cat">
                                {{this.Directions}}
                            </p>
                        </div>

                        <a href="https://www.behance.net/" class="item-folio__project-link" title="Project link">
                            <i class="icon-link"></i>
                        </a>

                        <div class="item-folio__caption">
                            <p>fwfwef</p>
                        </div>

                    </div>
                </div> <!-- end masonry__brick -->
                {{/each}}

            </div> <!-- end masonry -->
        </div> <!-- end col-full -->
    </div> <!-- end works-content -->

</section> <!-- end s-works -->

